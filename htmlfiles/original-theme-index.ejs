                    </div>
                </div>
                <div class="inputBox" id="inputBox">
                    <div class="input">
                        <form action="" id="chat-form" class="form">
                            <input type="hidden" value="<%=userId %>" id="userId" />
                            <input type="hidden" value="<%=User %>" id="User" />
                            <input type="hidden" value="global" id="socketId" />
                            <input type="hidden" value="/ProfilePic/<%=imageUrl %>" id="imageUrl" />
                            <input type="text" id="msg" form="chat-form" placeholder="Type something here" autofocus
                                autocomplete="off" class="msg">

                            <div class="usersTyping" id="usersTyping">...</div>

                            <button style="visibility: hidden;" type="submit" id="send-button">
                                Send
                            </button>
                        </form>
                    </div>
                    <div class="inputOthers">
                        <div class="inputOption">
                            <!-- <img src="/smiley-face.png" height="30px" width="30px" id="emoji-trigger" onmouseover="this.src='/smiley-face2.png'" onmouseout="this.src='/smiley-face.png'"> -->
