    <style>
        body {
            background-color: rgb(12, 4, 32);
            color: white;
            font-family: 'Josefin Sans', sans-serif;
        }

        .logoContainner {
            height: 90px;
            width: 70%;
            position: absolute;
            top: 50px;
            left: 50%;
            transform: translateX(-50%);

        }

        .logoBox {
            height: 100%;
            width: 100%;
        }

        #logo {
            fill: white;

        }

        #line {
            color: white;
        }

        .main {
            position: absolute;
            top: 200px;
            width: 90%;
            height: 500px;
            margin: auto;
            display: flex;
            justify-content: space-around;

        }

        .intro {
            height: 100%;
            flex: 1;
        }

        .info {
            margin: 30px;
            max-width: 500px;
        }

        .image {
            margin-top: -100px;
            padding: 10px;
            flex: 1;
            height: 100%;
            overflow: hidden;
            display: flex;
            justify-content: center;
            align-items: center;

        }

        .image img {
            border-radius: 10px;
            width: 280px;
            height: 280px;
            box-shadow: aqua 1px 1px 70px;
            transition: 0.6s;
        }

        .image:hover img {
            transition: 0.6s;
            box-shadow: crimson 1px 1px 70px;


        }

        #ball {
            fill: white;
            transition: 2s;
            animation: ballanimation infinite 1s ease-in-out;
        }

        @keyframes ballanimation {
            0% {
                transform: translateY(0px);

            }

            50% {
                transform: translateY(-30px);

            }

            100% {
                transform: translateY(0px);
            }
        }

        /* width */
        ::-webkit-scrollbar {
            width: 0px;
            border-radius: 5px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            background: gray;
            border-radius: 5px;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: white;
            border-radius: 5px;
        }

        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
            background: white;
        }

        button {
            height: 50px;
            width: 100px;
            background-color: crimson;
            outline: none;
            border-radius: 0%;
            border-style: none;
            font-size: 15px;
            color: white;
            font-weight: bold;
            transition: 0.5s;
            box-shadow: crimson 1px 1px 10px;
            cursor: pointer;

        }

        button:hover {
            background-color: rgb(76, 167, 167);
            box-shadow: rgb(43, 240, 141) 0px 5px 5px;
            transition: 0.5s;

        }

        @media screen and (max-width: 400px) {
            body {
