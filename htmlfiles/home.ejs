                display: flex;
                justify-content: center;

            }

            .logoBox {
                height: 100%;
                width: 100%;
            }

            #logo {
                fill: white;

            }

            #line {
                color: white;
            }

            .main {
                display: block;
                margin-top: 50px;
                width: 90%;
                height: 500px;
                margin: auto;

            }

            .intro {
                height: 100%;
                width: 90%;
            }

            .info {
                margin: 30px;
                max-width: 250px;
            }

            .image {

                padding: 10px;
                height: 100%;
                overflow: hidden;
                display: flex;
                justify-content: center;
                align-items: center;

            }

            .image img {
                border-radius: 10px;
                width: 240px;
                height: 240px;
                box-shadow: aqua 1px 1px 40px;
                transition: 0.6s;
            }

            .image:hover img {
                transition: 0.6s;
                box-shadow: crimson 1px 1px 40px;


            }

        }

        a {
            text-decoration: none;
            color: white;
        }
