    <main>

        <div class="containner" id="containner">
            <div class="roomList" id="roomList">
                <div class="userInfo" id="userInfo">

                    <div class="UserContainer">

                        <div class="userImage">
                            <img src="/ProfilePic/<%=imageUrl %>">
                        </div>
                        <br>
                        <div class="userUsername">
                            <h3><%=User %></h3>
                        </div>
                    </div>

                    <div class="logout">
                        <a href="/logout">
                            <i class="fa fa-sign-out" style="font-size:24px;"></i>
                        </a>

                    </div>
                </div>



            </div>

            <div class="mainBox" id="mainBox">
                <div class="message-box" id="chatBox-global" onclick="hideSideBars()">

                    <div class="usersTyping" id="usersTyping"></div>
                </div>


                <form action="" id="chat-form">
                    <input type="hidden" value="<%=User %>" id="User" />
                    <input type="hidden" value="/ProfilePic/<%=imageUrl %>" id="imageUrl" />
                    <input type="hidden" value="global" id="socketId" />
                    <div class="messageInputBox">

                        <div class="messageInputDiv">
                            <input type="text" class="message-input" id="msg" autofocus autocomplete="off" />

                        </div>
                        <div class="emojiDiv">
                            <img src="/smiley-face.png" height="30px" width="30px" id="emoji-trigger"
                                onmouseover="this.src='/smiley-face2.png'" onmouseout="this.src='/smiley-face.png'">
                        </div>



                    </div>
                    <button style="visibility: hidden;" type="submit" id="send-button">
                        Send
                    </button>
                </form>
            </div>

            <div class="onlineList" id="onlineList">
                <center>Online Users</center>
            </div>





        </div>

        <audio id="messageSound">
            <source src="/stairs.ogg" type="audio/ogg">
            <source src="/stairs.mp3" type="audio/mpeg">
            Your browser does not support the audio element.
        </audio>

    </main>



    <!-- ============================================================================================================================= -->

    <script>
        setTimeout(() => {
            document.getElementById('preLoaderDiv').style.display = 'none';
        }, 1);


    </script>






    <script>
        function toggleNightMode() {
            var body = document.body;
            body.classList.toggle('night-body');

            var messageBox = document.getElementById('message-box');
            messageBox.classList.toggle('night-message-box');

            var onlineList = document.getElementById('onlineList');
            onlineList.classList.toggle('night-onlineList');

            var roomList = document.getElementById('roomList');
            roomList.classList.toggle('night-roomList');

            var containner = document.getElementById('containner');
            containner.classList.toggle('night-containner');


        }

    </script>



    <script src="https://cdn.jsdelivr.net/npm/emoji-button@latest/dist/index.min.js"></script>
    <script>
        const picker = new EmojiButton();
        const trigger = document.querySelector('#emoji-trigger');

        picker.on('emoji', selection => {
            // handle the selected emoji here
            document.getElementById('msg').value += selection;
            document.getElementById('msg').focus();
        });

        trigger.addEventListener('click', function () {
            picker.pickerVisible ? picker.hidePicker() : picker.showPicker(trigger)
        });

    </script>

    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/main.js"></script>


    <script src="/js/swipe.js"></script>

    <!-- ======================================================================================================================= -->
    <script src="https://js.pusher.com/4.0/pusher.min.js"></script>


</body>

</html>